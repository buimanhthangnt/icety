{
    "manifest_version": 2,
    "name": "New Tab",
    "description": "Make boring things less boring :D",
    "version": "1.0.2",
    "author": "Bùi Mạnh Thắng",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "New Tab",
        "icons": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "permissions": ["activeTab"]
}